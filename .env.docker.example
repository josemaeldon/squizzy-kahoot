# Example environment variables for Docker deployment
# Copy this file to .env and fill in your actual values

# Sanity.io Write Token (REQUIRED)
# Get this from: https://manage.sanity.io/projects/YOUR_PROJECT_ID/settings/api
SQUIZZY_WRITE_TOKEN=your-sanity-write-token-here

# Application Port (default: 3000)
PORT=3000

# Node Environment (production or development)
NODE_ENV=production
